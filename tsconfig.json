{
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs",
    }
  },
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": false,
    "declaration": false,
    "baseUrl": "./",
    "strict": false,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "src/*",
      ],
      "@@/*": [
        "src/.umi/*"
      ]
    },
    "outDir": "dist",
    "rootDir": "./src",
    "allowSyntheticDefaultImports": true,
    "removeComments": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "lib",
    "es",
    "dist",
    "typings",
    "**/__test__",
    "test"
  ]
}